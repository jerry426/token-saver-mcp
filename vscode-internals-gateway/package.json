{
  "name": "vscode-internals-gateway",
  "displayName": "VSCode Internals Gateway",
  "version": "2.2.0",
  "description": "Complete programmatic control of VSCode via HTTP - expose ALL VSCode internals to external processes",
  "publisher": "jerry426",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": []
  },
  "enabledApiProposals": [
    "findTextInFiles"
  ],
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "node build.js",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "22.x",
    "@types/vscode": "^1.97.0",
    "esbuild": "^0.25.9",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2"
  }
}
